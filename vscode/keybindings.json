// The key bindings defined using File > Preferences > Keyboard Shortcuts menu path.
// Note this is the version that I am using on Linux (Xubuntu, if you're curious :)).
[
    {
        "key": "ctrl+alt+1",
        "command": "workbench.view.explorer"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "ctrl+alt+5",
        "command": "workbench.view.extensions"
    },
    {
        "key": "ctrl+shift+x",
        "command": "-workbench.view.extensions"
    },
    {
        "key": "ctrl+alt+4",
        "command": "workbench.view.debug"
    },
    {
        "key": "ctrl+shift+d",
        "command": "-workbench.view.debug"
    },
    {
        "key": "ctrl+alt+2",
        "command": "workbench.view.search",
        "when": "!searchViewletVisible"
    },
    {
        "key": "ctrl+shift+f",
        "command": "-workbench.view.search",
        "when": "!searchViewletVisible"
    },
    {
        "key": "ctrl+alt+3",
        "command": "workbench.scm.focus"
    }
]
